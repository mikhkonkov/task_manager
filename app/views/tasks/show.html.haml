- provide(:title, @task.title)
.panel.panel-primary
  .panel-heading
    %h3.panel-title
      = @task.title
  .panel-body
    = @task.description
    %p.text-right
      %strong assigned to:
      = @task.assigned_to.name
    %p.text-right
      %strong creator:
      = @task.creator.name
    %p.text-right
      %strong state:
      = @task.state
